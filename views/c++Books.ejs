    <br>
    <br>
    <br>
    
    <div class='headingBooks'>
    <h2>C/C++ books</h2>
    </div>


    <!-- books cards horror types -->
    <div class="row row-cols-1 row-cols-md-2 g-4">
        <% for(var i=0;i<cBooks.length;i++){ %>
        <div class="col-6 col-md-3">
            <a href='/coding/c/<%= cBooks[i]._id%>'>
                <div class="card">
                    <img src="<%= cBooks[i].image %>" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title"><%= cBooks[i].name %></h5>
                    </div>
                </div>
            </a>
        </div>
        <% } %>
    </div>

    

   

