                <form class="d-flex " action='searchBooks' method='GET'>
                    <input class="form-control me-2" type="search" placeholder="enter book name" name="searchItemName"
                        aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
            </div>
        </div>
